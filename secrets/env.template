# Secrets Configuration Template
# WARNING: This file contains sensitive information
# DO NOT commit this file to version control
# Copy this file to secrets/.env and fill in your actual values

# Database Secrets
DATABASE_ROOT_PASSWORD=your_secure_root_password_here
DATABASE_USER=uncloud_user
DATABASE_PASSWORD=your_secure_database_password_here

# Email Service Secrets
EMAIL_SMTP_PASSWORD=your_smtp_password_here
EMAIL_IMAP_PASSWORD=your_imap_password_here
EMAIL_DOMAIN=yourdomain.com

# Password Manager Secrets
PASSWORDMANAGER_ADMIN_PASSWORD=your_admin_password_here
PASSWORDMANAGER_SECRET_KEY=your_secret_key_here

# SSL/TLS Secrets
SSL_PRIVATE_KEY_PATH=/secrets/ssl/private.key
SSL_CERTIFICATE_PATH=/secrets/ssl/certificate.crt

# API Keys and Tokens
DYNAMIC_DNS_API_KEY=your_dynamic_dns_api_key_here
MONITORING_API_KEY=your_monitoring_api_key_here

# Backup Secrets
BACKUP_ENCRYPTION_KEY=your_backup_encryption_key_here
BACKUP_S3_ACCESS_KEY=your_s3_access_key_here
BACKUP_S3_SECRET_KEY=your_s3_secret_key_here

# Load Balancer Secrets
LOADBALANCER_BASIC_AUTH=your_basic_auth_credentials_here

# DNS Secrets
DNS_API_KEY=your_dns_api_key_here
DNS_ZONE_ID=your_zone_id_here

# Traefik Secrets
TRAEFIK_DASHBOARD_PASSWORD=your_traefik_dashboard_password_here
TRAEFIK_API_TOKEN=your_traefik_api_token_here

# Service-specific Secrets
SYNCSERVICE_ENCRYPTION_KEY=your_sync_encryption_key_here
WEBSITE_ADMIN_PASSWORD=your_website_admin_password_here
NAS_ACCESS_TOKEN=your_nas_access_token_here 